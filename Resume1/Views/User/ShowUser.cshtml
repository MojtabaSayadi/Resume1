@using Resume1.domain.Models.Auth;

@model List<User>

<div>
	<table>
		<tr>
			<th>
				#
			</th>
			<th>
				FullName
			</th>
			<th>
				Password
			</th>
		</tr>
		@if(Model.Any())
		{
			foreach (var item in Model)
			{
				<tr>
					<td>
					  @item.Id
					</td>
					<td>
						@item.FullName
					</td>
					<td>
						@item.Password
					</td>
					
				</tr>
				}
			}
			else
			{
				<tr>
					<td colspan="3">
						No Data Found

					</td>
				</tr>
			}
		</table>

	</div>

 
